{
  "expo": {
    "newArchEnabled": true,
    "owner": "andre_luis",
    "name": "nep-corte",
    "slug": "nepcorte",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EC441E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "br.dev.ness.nepcorte",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "googleServicesFile": "./google-services.json",
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "package": "br.dev.ness.nepcorte",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#EC441E"
      },
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ab899fe7-fb70-4030-a3f0-290b5861abfe"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Dar ao NepCorte o acesso a c√¢mera",
          "microphonePermission": "Dar ao NepCorte o acesso ao microfone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
